<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.findactivitypartner.damienaxel.findactivitypartner_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sport14"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserHomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailAndResultCardActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".CreateUserActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".CreateNewCardActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ResultCardActivity"
            android:screenOrientation="portrait"/>


    </application>

</manifest>